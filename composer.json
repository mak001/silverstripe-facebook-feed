{
	"name": "mak001/silverstripe-facebook-feed",
	"description": "SilverStripe Facebook Feed Module. Show a feed from FaceBook.",
	"keywords": [
		"silverstripe",
		"facebook",
		"facebook feed"
	],
	"type": "silverstripe-vendormodule",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "mak001",
			"email": "mak001001@gmail.com",
			"homepage": "http://matthewkoerber.com"
		}
	],
	"require": {
		"silverstripe/framework": "^4.0",
		"silverstripe/cms": "^4.0",
		"facebook/graph-sdk": "^5.6"
	},
	"require-dev": {
		"phpunit/PHPUnit": "^5.7",
		"squizlabs/php_codesniffer": "*"
	},
	"extra": {
		"branch-alias": {
			"1.0": "1.0.x-dev",
			"dev-master": "2.0.x-dev"
		}
	},
	"autoload": {
		"psr-4": {
			"Mak001\\FacebookFeed\\": "src/",
			"Mak001\\FacebookFeed\\Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"lint": "vendor/bin/phpcs src/ tests/",
		"lint-clean": "vendor/bin/phpcbf src/ tests/"
	}
}
